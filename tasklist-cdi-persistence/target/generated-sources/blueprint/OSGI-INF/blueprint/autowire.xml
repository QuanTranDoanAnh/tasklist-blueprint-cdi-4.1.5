<?xml version="1.0" encoding="UTF-8"?><blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">
    <bean xmlns:ext="http://aries.apache.org/blueprint/xmlns/blueprint-ext/v1.0.0" id="initHelper" class="vn.quantda.tasklist.cdi.persistence.impl.InitHelper" ext:field-injection="true" scope="prototype" init-method="addDemoTasks">
        <property name="taskService" ref="taskServiceImpl"/>
    </bean>
    <bean id="taskServiceImpl" class="vn.quantda.tasklist.cdi.persistence.impl.TaskServiceImpl" scope="prototype"/>
    <service ref="taskServiceImpl" interface="vn.quantda.tasklist.cdi.model.TaskService">
        <service-properties>
            <entry key="service.exported.interfaces" value="*"/>
        </service-properties>
    </service>
    <enable xmlns="http://aries.apache.org/xmlns/jpa/v2.0.0"/>
    <enable xmlns="http://aries.apache.org/xmlns/transactions/v2.0.0"/>
</blueprint>
